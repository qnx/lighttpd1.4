<?xml version="1.0" encoding="UTF-8"?>
<shiplist>
	<id>com.qnx.${baselineId}.osr.lighttpd.internal</id>
	<name>Lighttpd 1.4.71 web server</name>
	<description>Lighttpd is a secure, fast, compliant, and very flexible web server</description>
	<property key="f.support" value="Custom"/>
	<property key="f.swtype" value="III"/>
	<property key="f.engage" value="B"/>
	<category>OSR</category>

	<property key="com.qnx.internal" value="true"/>
	<!-- don't generate provides/requires tags for internal packages-->
	<requires value="suppress-library: autogen"/>
	<provides value="suppress-library: autogen"/>

	<asset dest="${targetfolder}/[ARCH:aarch64le,x86_64]/usr/sbin/lighttpd" runtime="false" />
	<asset dest="${targetfolder}/[ARCH:aarch64le,x86_64]/usr/sbin/lighttpd-angel" runtime="false" />
</shiplist>
